{
  "name": "d3-graph-controller",
  "version": "1.2.0",
  "license": "MIT",
  "repository": "github:DerYeger/d3-graph-controller",
  "homepage": "https://graph-controller.yeger.eu",
  "bugs": {
    "url": "https://github.com/DerYeger/d3-graph-controller/issues"
  },
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "keywords": [
    "d3",
    "graph",
    "controller"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf ./dist",
    "build": "vite build",
    "demo:dev": "vite --config demo/vite.config.ts",
    "demo:build": "vite --config demo/vite.config.ts build",
    "demo:preview": "yarn demo:build && vite --config demo/vite.config.ts preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "yarn docs:build && vitepress serve docs",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:commits",
    "lint:commits": "yarn commitlint --from=master",
    "lint:eslint": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:prettier": "prettier --check \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "test": "vitest",
    "test:ci": "rimraf ./coverage && c8 --reporter=lcov --reporter=text vitest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "default.css"
  ],
  "main": "./dist/d3-graph-controller.umd.js",
  "module": "./dist/d3-graph-controller.es.js",
  "exports": {
    ".": {
      "import": "./dist/d3-graph-controller.es.js",
      "require": "./dist/d3-graph-controller.umd.js"
    },
    "./default.css": {
      "import": "./default.css",
      "require": "./default.css"
    }
  },
  "types": "dist/types/src/main.d.ts",
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/d3": "7.1.0",
    "@types/node": "16.11.15",
    "@types/resize-observer-browser": "0.1.6",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vitejs/plugin-vue": "2.0.1",
    "c8": "^7.10.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.2.0",
    "husky": "7.0.4",
    "jsdom": "19.0.0",
    "lint-staged": "12.1.3",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.4",
    "unocss": "0.17.3",
    "unplugin-vue-components": "0.17.9",
    "vite": "2.7.6",
    "vite-plugin-dts": "0.9.6",
    "vitepress": "0.20.9",
    "vitest": "0.0.109",
    "vue": "3.2.26",
    "vue-eslint-parser": "8.0.1"
  },
  "dependencies": {
    "d3": "7.2.1",
    "ml-matrix": "6.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint"
  }
}
