{
  "name": "d3-graph-controller",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "github:DerYeger/d3-graph-controller",
  "homepage": "https://graph-controller.yeger.eu",
  "bugs": {
    "url": "https://github.com/DerYeger/d3-graph-controller/issues"
  },
  "author": {
    "name": "Jan MÃ¼ller",
    "url": "https://github.com/DerYeger"
  },
  "keywords": [
    "d3",
    "graph",
    "controller"
  ],
  "scripts": {
    "postinstall": "yarn build",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postbuild": "yalc push",
    "demo:serve": "yarn build && vite --config demo/vite.config.ts",
    "demo:build": "yarn build && vite --config demo/vite.config.ts build",
    "demo:preview": "yarn demo:build && vite --config demo/vite.config.ts preview",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:prettier": "prettier --check \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "default.css"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./default.css": {
      "import": "./default.css"
    }
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/d3": "7.1.0",
    "@types/node": "16.11.15",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vitejs/plugin-vue": "2.0.1",
    "d3-graph-controller": "file:.",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.2.0",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "vite": "2.7.6",
    "vue": "3.2.26",
    "vue-eslint-parser": "8.0.1",
    "yalc": "1.0.0-pre.53"
  },
  "dependencies": {
    "d3": "7.2.1",
    "ml-matrix": "6.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
